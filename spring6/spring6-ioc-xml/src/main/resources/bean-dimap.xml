<?xml version="1.0"  encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!--    1/创建两个对象
        2/注入普通类型属性
        3/在学生bean注入map集合类型属性

-->
    <bean id="teacherone" class="com.atguigu.spring6.iocxml.dimap.Teacher">
<!--        /注入普通类型属性-->
        <property name="teacherId" value="100"></property>
        <property name="teacherName" value="YUXSWW"></property>
    </bean>
    <bean id="teachertwo" class="com.atguigu.spring6.iocxml.dimap.Teacher">
        <!--        /注入普通类型属性-->
        <property name="teacherId" value="256"></property>
        <property name="teacherName" value="iuuihiiu"></property>
    </bean>
    <bean id="student" class="com.atguigu.spring6.iocxml.dimap.Student">
        <!--        /注入普通类型属性-->
        <property name="sid" value="1005466"></property>
        <property name="sname" value="YWYEWSS"></property>
<!--        3/在学生bean注入map集合类型属性-->
        <property name="teacherMap">
            <map>
                <entry>
                    <key>
                        <value>1000</value>
                    </key>
                    <ref bean="teacherone"></ref>
                </entry>
                <entry>
                    <key>
                        <value>10085</value>
                    </key>
                    <ref bean="teachertwo"></ref>
                </entry>
            </map>
        </property>
    </bean>

</beans>